{
  "name": "st-datepicker",
  "version": "0.0.8",
  "description": "A web datepicker component",
  "main": "dist/st-datepicker.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "keywords": [
    "Web Component",
    "Stencil",
    "datepicker",
    "date-picker",
    "calendar",
    "Angular",
    "Vue",
    "React",
    "Javascript"
  ],
  "files": [
    "dist/"
  ],
  "browser": "dist/st-datepicker.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "^0.6.18",
    "hammerjs": "^2.0.8",
    "moment": "^2.21.0"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/sass": "^0.0.3",
    "@stencil/utils": "latest",
    "@types/hammerjs": "^2.0.35",
    "@types/jest": "^21.1.1",
    "@types/moment": "^2.13.0",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fausfore/st-datepicker.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil"
  },
  "homepage": "https://github.com/ionic-team/stencil",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
