  "devDependencies": {
    "@stencil/core": "~0.16.2",
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.12.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "puppeteer": "1.8.0"
  },
  "license": "MIT"
}
